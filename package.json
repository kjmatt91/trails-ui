{
  "name": "trails-api",
  "version": "1.0.0",
  "description": "An API that will allow users to search for nearby trails by location and type of trail.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "npm run dev:client:build && ./node_modules/.bin/env-cmd ./node_modules/.bin/nodemon server.js",
    "dev:migrate": "./node_modules/.bin/env-cmd ./node_modules/.bin/sequelize db:migrate --env development --config ./config/sequelize.js",
    "dev:client": "cd client && npm start",
    "dev:client:build": "cd client && npm run build",
    "lint": "./node_modules/.bin/eslint --format codeframe .",
    "lint:fix": "./node_modules/.bin/eslint --fix --format codeframe .",
    "dev:client:test": "cd client && npm run test",
    "dev:server:test": "./node_modules/.bin/env-cmd ./node_modules/.bin/mocha tests/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjmatt91/trails-api.git"
  },
  "author": "Kaleb Mattison",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kjmatt91/trails-api/issues"
  },
  "homepage": "https://github.com/kjmatt91/trails-api#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mysql2": "^2.3.2",
    "pug": "^3.0.2",
    "sequelize": "^6.7.0",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "concurrently": "^6.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.11.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.13",
    "pug-lint": "^2.6.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0"
  }
}
